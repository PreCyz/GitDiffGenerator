<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="210.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TitledPane animated="false" collapsible="false" layoutX="10.0" layoutY="14.0" text="%job.clickWay.title">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="120.0" prefWidth="378.0">
               <children>
                  <RadioButton fx:id="everyMonthRadioButton" layoutX="14.0" layoutY="13.0" mnemonicParsing="false" text="%job.clickWay.everyMonth">
                     <toggleGroup>
                        <ToggleGroup fx:id="triggerOptions" />
                     </toggleGroup>
                  </RadioButton>
                  <ComboBox fx:id="dayNameComboBox" layoutX="214.0" layoutY="10.0" prefWidth="150.0" />
                  <RadioButton fx:id="every2WeeksRadioButton" layoutX="14.0" layoutY="51.0" mnemonicParsing="false" text="%job.clickWay.every2Weeks" toggleGroup="$triggerOptions" />
                  <RadioButton fx:id="everyWeekRadioButton" layoutX="14.0" layoutY="89.0" mnemonicParsing="false" selected="true" text="%job.clickWay.everyWeek" toggleGroup="$triggerOptions" />
               </children>
            </AnchorPane>
        </content>
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </TitledPane>
      <TitledPane animated="false" collapsible="false" layoutX="394.0" layoutY="14.0" prefHeight="147.0" prefWidth="315.0" text="%job.cronWay.title">
         <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="130.0" prefWidth="195.0">
               <children>
                  <TextField fx:id="cronExpressionTextField" layoutX="131.0" layoutY="44.0" />
                  <Label layoutX="14.0" layoutY="49.0" text="%job.cronWay.cronExp" />
               </children>
            </AnchorPane>
         </content>
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </TitledPane>
      <Button layoutX="343.0" layoutY="166.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="95.0" text="%job.schedule" textFill="#24bf31">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
